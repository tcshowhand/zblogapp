{
    "pages": [
        "pages/home/index",
        "pages/list/index",
        "pages/article/index",
        "pages/page/index",
        "pages/search/index",
        "pages/about/index",
        "pages/sorts/index",
        "pages/smtform/index",
        "pages/login/index",
        "pages/asklist/index",
        "pages/askinfo/index",
        "pages/kong/index",
        "pages/konginfo/index",
        "swan-sitemap/index"
    ],
    "window": {
        "backgroundTextStyle": "dark",
        "navigationBarBackgroundColor": "#338ff0",
        "navigationBarTextStyle": "white"
    },
    "dynamicLib": {
        "myDynamicLib": {
            "provider": "swan-interaction"
        },
        "swan-sitemap-lib": {
            "provider": "swan-sitemap"
        },
        "swanGov": {
            "provider": "swan-gov"
        },
        "swanKit": {
            "provider": "swan-serverless-kit"
        }
    },
    "tabBar": {
        "color": "#80848f",
        "selectedColor": "#338ff0",
        "backgroundColor": "#f7f7fa",
        "list": [
            {
                "pagePath": "pages/home/index",
                "iconPath": "images/icon/Planet.png",
                "selectedIconPath": "images/icon/Planet_fill.png",
                "text": "首页"
            },
            {
                "pagePath": "pages/sorts/index",
                "iconPath": "images/icon/luyou.png",
                "selectedIconPath": "images/icon/luyou_fill.png",
                "text": "分类"
            },
            {
                "pagePath": "pages/search/index",
                "iconPath": "images/icon/fenxiang.png",
                "selectedIconPath": "images/icon/fenxiang_fill.png",
                "text": "搜索"
            },
            {
                "pagePath": "pages/about/index",
                "iconPath": "images/icon/daima.png",
                "selectedIconPath": "images/icon/daima_fill.png",
                "text": "个人中心"
            }
        ]
    },
    "requiredBackgroundModes": ["audio"],
    "networkTimeout": {
        "request": 30000,
        "connectSocket": 10000,
        "uploadFile": 10000,
        "downloadFile": 10000
    },
    "permission": {
        "scope.userLocation": {
            "desc": "你的位置信息将用于小程序位置接口的效果展示"
        }
    },
    "prefetch" : {
        "enabled": true
    },
    "cookie": {
        "enableStore": true
    },
    "useSwanNews": true,
    "debug": true
}